{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "cacheDir": ".turbo/cache",
  "globalDependencies": ["**/.env"],
  "globalEnv": [
    "NEXT_PUBLIC_API_URL",
    "NODE_ENV",
    "REDIS_URL",
    "REDIS_TOKEN",
    "RATE_LIMIT_TTL",
    "RATE_LIMIT_MAX_REQUESTS",
    "RATE_LIMIT_STORAGE",
    "ENCRYPTION_KEY",
    "GOOGLE_GENERATIVE_AI_API_KEY",
    "WORKFLOWS_SERVICE_HOST",
    "WORKFLOWS_SERVICE_PORT",
    "EXECUTION_SERVICE_HOST",
    "EXECUTION_SERVICE_PORT",
    "USERS_SERVICE_HOST",
    "USERS_SERVICE_PORT",
    "COMPONENTS_SERVICE_HTTP_PORT",
    "COMPONENTS_SERVICE_PORT",
    "COMPONENTS_SERVICE_HOST",
    "ANALYTICS_SERVICE_HOST",
    "ANALYTICS_SERVICE_PORT",
    "PROJECTS_SERVICE_HOST",
    "PROJECTS_SERVICE_PORT",
    "BILLING_SERVICE_HOST",
    "BILLING_SERVICE_PORT",
    "AUTH_SERVICE_HOST",
    "AUTH_SERVICE_PORT",
    "API_VERSION",
    "SSL_KEY_PATH",
    "SSL_CERT_PATH",
    "CACHE_DISABLED"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "type-check": {
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
